{
  "name": "node_api_setup",
  "version": "1.0.0",
  "description": "Basic api with node, ts, postgres",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "prestart": "cross-env NODE_ENV=production && npm run remove-build && npm run tsc && npm run build-client",
    "start": " node ./build/index.js",
    "tsc:watch": "tsc --watch",
    "server:watch": "nodemon ./build/index.js",
    "devPrepared": "cross-env NODE_ENV=development && npm run remove-build && npm run tsc",
    "start:dev": "npm run devPrepared && npm-run-all --parallel tsc:watch server:watch client:watch",
    "build-client": "webpack",
    "remove-build": "rimraf ./build",
    "client:watch": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damiench/node_api-setup.git"
  },
  "author": "damiench",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/damiench/node_api-setup/issues"
  },
  "homepage": "https://github.com/damiench/node_api-setup#readme",
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.5",
    "@types/express": "^4.0.37",
    "@types/express-session": "^1.15.3",
    "@types/lodash": "^4.14.76",
    "@types/passport": "^0.3.4",
    "@types/passport-local": "^1.0.31",
    "@types/pg": "^7.1.2",
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/react-redux": "^5.0.10",
    "@types/react-router-dom": "^4.0.8",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "@types/multer": "^1.3.3",
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "^3.2.3",
    "axios": "^0.16.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.4",
    "multer": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2"
  }
}
